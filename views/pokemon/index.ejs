<%- include('../partials/header') %>
<div>

    <% pokemon.forEach(function(p) { %>
        <div><%= p.name %></div>
        <div>
           <img src="<%= p.img %>" alt="Pic"> 
        </div>
        <div><%= p.type %></div>
        <form action="/pokemon/<%= p._id %>?_method=DELETE" method="POST"> 
            <button>Remove</button>
        </form>
    <% }); %>
</div>
<h3>efefe</h3>

